<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gyaku.bearschool.admin.adminserver.dao.UserDao">

    <select id="find" resultType="gyaku.bearschool.common.servercommon.domain.User" parameterType="Map">
        select * from user
        <if test="id != null and id != ''">
            where id = #{id}
        </if>
    </select>

    <select id="findAll" resultType="gyaku.bearschool.common.servercommon.domain.User">
        select * from user
    </select>

</mapper>